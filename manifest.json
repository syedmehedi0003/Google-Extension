{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage"
    ],
    "chrome_url_overrides": {
        "tab": "clock.html"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "logo16.png",
            "32": "logo32.png",
            "48": "logo48.png"
        }
    }
}